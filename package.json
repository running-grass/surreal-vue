{
  "name": "surreal-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p startdb startVite",
    "build": "vue-tsc && vite build",
    "startdb": "surreal start --log debug --user root --pass root memory",
    "initdb": "cat init.sql | surreal sql --conn http://localhost:8000 --user root --pass root --ns test --db test",
    "startVite": "vite",
    "startAndInitDB": "run-p startdb initdb",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.2.19",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.23",
    "surrealdb.js": "^0.5.0",
    "vue": "^3.2.41",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9"
  }
}